public class Desenvolvedor {
    private String nome;
    private int codigo = 0;
    static int qtd_obj;

    Desenvolvedor(){
        qtd_obj++;
        this.codigo = qtd_obj;
    }

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public int getCodigo() {
        return codigo;
    }

    public void setCodigo(int codigo) {
        this.codigo = codigo;
    }

    
    
}
